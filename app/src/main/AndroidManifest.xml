<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.swufe.weaw">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".base.UniteApp"
        android:allowBackup="true"
        android:label="WEAW"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:icon="@mipmap/icon"
        android:usesCleartextTraffic="true">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DeleteCityActivity" />
        <activity android:name=".SearchCityActivity" />
        <activity android:name=".MainActivity">

        </activity>
        <activity android:name=".city_manager.CityManagerActivity" />
        <activity android:name=".city_manager.SearchCityActivity" />
    </application>

</manifest>